<h1 class="center ">Personal Settings</h1>

<div class="col-md-6 col-md-offset-3">
  <%= form_for @user do |f| %>
    <%= f.text_field :first_name, autofocus: true, placeholder: "First Name", class: "girls-input" %>
    <%= f.text_field :last_name, placeholder: "Last Name", class: "girls-input" %>
    <%= f.submit "Update Profile", class: "btn submit-btn" %>
  <% end %>
</div>

<div class="col-md-12 center">
  <%= link_to "Back", :back %>
</div>
